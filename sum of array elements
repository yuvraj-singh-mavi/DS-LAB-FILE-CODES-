#include<stdio.h>

void main()
{
    int arr[10] = {10, 20, 30, 1, 2, 3, 4, 5, 6}, size, i, sum = 0;


    size = sizeof(arr) / sizeof(arr[0]);

    printf("Enter array elements\n");
    for (i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    
    sum = arr[0] + arr[size - 1];

    printf("Sum of the elements = %d\n", sum);
}
